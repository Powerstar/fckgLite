<Styles>


      <Style name="Create TH" element="span">
		<Attribute name="th" value="th" />
		<Style name="background-color" value="#dee7ec" />
		<!--  Style name="font-weight" value="bold" /  -->
	</Style>

       <Style name="Remove TH" element="span">
		<Attribute name="th" value="del" />
		<Style name="background-color" value="white" />
	</Style>


	<Style name="Code" element="pre">
	</Style>          
        
	<Style name="Strong Emphasis" element="strong" />
	<Style name="Emphasis" element="em" />

	<Style name="Typewriter" element="tt">
           <Style name="font-family" value="Courier New, Monospace" />	
 	   <Style name="font-size" value="120%" />	 
	</Style>

	<Style name="Keyboard Phrase" element="kbd" />
	
	<Style name="Sample Text" element="samp" >
	  <Style name="font-size" value="120%" />	 
	  <Style name="font-family" value="sans-serif" />	 
	</Style>
	
	<Style name="Variable" element="var" />

	<!-- Style name="Deleted Text" element="del" /  -->
	<Style name="Inserted Text" element="u" />

	<Style name="Cited Work" element="cite" />
	



</Styles>
